#Rewrite rules
<IfModule mod_rewrite.c>
	XBitHack on
	Options +Includes +FollowSymlinks
	RewriteEngine On
	RewriteBase /
	RewriteCond %{REQUEST_FILENAME} -s [OR]
	RewriteCond %{REQUEST_FILENAME} -l [OR]
	RewriteCond %{REQUEST_FILENAME} -d
	RewriteRule ^.*$ - [NC,L]
	RewriteRule ^.*$ index.php [NC,L]	
</IfModule>

